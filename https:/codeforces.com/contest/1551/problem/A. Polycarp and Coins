#include <bits/stdc++.h>
 
using namespace std;
#define lli long long int
void solve()
{
   lli n; cin>>n;
   if(n%3==0){
       cout<< n/3 <<" "<< n/3 <<endl; return;
   }
   lli a=n/3,b=a+1;
   if(a + (2*b) == n)
   {
       cout<<a<<" "<<b<<endl; return;
   }
   else
   {
       cout<<b<<" "<<a<<endl;
   }
}
 
int main()
{
    int t; cin>>t;
    while(t--)
    {
        solve();
    }
 
    return 0;
}
