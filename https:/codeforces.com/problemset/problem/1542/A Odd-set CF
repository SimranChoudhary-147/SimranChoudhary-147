#include <bits/stdc++.h>

using namespace std;

int main()
{
   int t;
   cin>>t;
   while(t--)
   {
       int n;
       cin>>n;
       int x=2*n;
       int a[x]; int ce=0,co=0;
       for(int i=0;i<x;i++)
       {
           cin>>a[i];
           if(a[i] %2 ==0) ce++;
           else co++;
       }

       if(co==ce) 
            cout<<"Yes\n";
       else 
            cout<<"No\n";
   }

    return 0;
}
