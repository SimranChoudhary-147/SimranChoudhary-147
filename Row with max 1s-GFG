class Solution{
public:
	int rowWithMax1s(vector<vector<int> > arr, int n, int m) {
	    int ind=-1;
	    int maxi=0;
	    for(int i=0;i<n;i++)
	    {
	        int c=0;
	        for(int j=0;j<m;j++)
	        {
	            if(arr[i][j]==1)
	                c++;
	        }
	        if(maxi<c)
	        {
	            maxi=c;
	            ind=i;
	        }
	        
	    }
	    return ind;
	}

};
